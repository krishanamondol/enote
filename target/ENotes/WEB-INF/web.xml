<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.enotes.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.enotes.servlet.LoginServlet</servlet-class>
